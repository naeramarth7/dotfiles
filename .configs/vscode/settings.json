{
  "editor.disableTranslate3d": true,
  "editor.fontFamily": "\"Input Mono\"",
  "editor.fontWeight": "normal",
  "editor.fontSize": 11,
  "editor.lineHeight": 0,
  "editor.lineNumbers": "on",
  "editor.rulers": [
    80,
    100
  ],
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": true,
  "editor.roundedSelection": false,
  "editor.scrollBeyondLastLine": true,
  "editor.wordWrap": "off",
  "editor.wrappingIndent": "indent",
  "editor.quickSuggestions": true,
  "editor.quickSuggestionsDelay": 50,
  "editor.cursorBlinking": "solid",
  "editor.cursorStyle": "line",
  "editor.renderWhitespace": "all",
  "editor.renderIndentGuides": true,
  "editor.renderLineHighlight": "all",
  "workbench.editor.showIcons": true,
  "workbench.editor.enablePreview": true,
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "workbench.statusBar.visible": true,
  "workbench.activityBar.visible": true,
  "window.openFilesInNewWindow": "off",
  "window.zoomLevel": 0,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/bower_components": true,
    "**/coverage": true,
    "**/*.map": true,
    "**/node_modules": true
  },
  "files.trimTrailingWhitespace": false,
  "files.insertFinalNewline": true,
  "files.autoSave": "off",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true,
    "**/bower_components/**": true
  },
  "files.hotExit": "onExitAndWindowClose",
  "explorer.openEditors.visible": 0,
  "git.autorefresh": true,
  "git.autofetch": false,
  "css.validate": true,
  "css.lint.compatibleVendorPrefixes": "ignore",
  "css.lint.vendorPrefix": "warning",
  "css.lint.duplicateProperties": "error",
  "css.lint.emptyRules": "warning",
  "css.lint.importStatement": "ignore",
  "css.lint.boxModel": "ignore",
  "css.lint.universalSelector": "warning",
  "css.lint.zeroUnits": "ignore",
  "css.lint.fontFaceProperties": "warning",
  "css.lint.hexColorLength": "error",
  "css.lint.argumentsInColorFunction": "error",
  "css.lint.unknownProperties": "warning",
  "css.lint.ieHack": "error",
  "css.lint.unknownVendorSpecificProperties": "ignore",
  "css.lint.propertyIgnoredDueToDisplay": "warning",
  "css.lint.important": "warning",
  "css.lint.float": "ignore",
  "css.lint.idSelector": "ignore",
  "scss.validate": true,
  "scss.lint.compatibleVendorPrefixes": "ignore",
  "scss.lint.vendorPrefix": "warning",
  "scss.lint.duplicateProperties": "error",
  "scss.lint.emptyRules": "warning",
  "scss.lint.importStatement": "ignore",
  "scss.lint.boxModel": "ignore",
  "scss.lint.universalSelector": "warning",
  "scss.lint.zeroUnits": "ignore",
  "scss.lint.fontFaceProperties": "warning",
  "scss.lint.hexColorLength": "error",
  "scss.lint.argumentsInColorFunction": "error",
  "scss.lint.unknownProperties": "warning",
  "scss.lint.ieHack": "error",
  "scss.lint.unknownVendorSpecificProperties": "ignore",
  "scss.lint.propertyIgnoredDueToDisplay": "warning",
  "scss.lint.important": "warning",
  "scss.lint.float": "ignore",
  "scss.lint.idSelector": "ignore",
  "less.validate": true,
  "less.lint.compatibleVendorPrefixes": "ignore",
  "less.lint.vendorPrefix": "warning",
  "less.lint.duplicateProperties": "error",
  "less.lint.emptyRules": "warning",
  "less.lint.importStatement": "ignore",
  "less.lint.boxModel": "ignore",
  "less.lint.universalSelector": "warning",
  "less.lint.zeroUnits": "ignore",
  "less.lint.fontFaceProperties": "warning",
  "less.lint.hexColorLength": "error",
  "less.lint.argumentsInColorFunction": "error",
  "less.lint.unknownProperties": "warning",
  "less.lint.ieHack": "error",
  "less.lint.unknownVendorSpecificProperties": "ignore",
  "less.lint.propertyIgnoredDueToDisplay": "warning",
  "less.lint.important": "warning",
  "less.lint.float": "ignore",
  "less.lint.idSelector": "ignore",
  "html.format.wrapLineLength": 120,
  "html.format.unformatted": "a, abbr, acronym, b, bdo, big, br, button, cite, code, dfn, em, i, img,input, kbd, label, map, object, q, samp, script, select, small, span, strong, sub, sup, textarea, tt, var",
  "html.format.indentInnerHtml": true,
  "html.format.preserveNewLines": true,
  "html.format.maxPreserveNewLines": null,
  "html.format.indentHandlebars": true,
  "html.format.endWithNewline": true,
  "html.format.extraLiners": "head, body, /html",
  "html.format.wrapAttributes": "force",
  "typescript.tsdk": null,
  "typescript.tsserver.trace": "off",
  "typescript.validate.enable": true,
  "typescript.format.insertSpaceAfterCommaDelimiter": true,
  "typescript.format.insertSpaceAfterSemicolonInForStatements": true,
  "typescript.format.insertSpaceBeforeAndAfterBinaryOperators": true,
  "typescript.format.insertSpaceAfterKeywordsInControlFlowStatements": true,
  "typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": false,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": false,
  "typescript.format.placeOpenBraceOnNewLineForFunctions": false,
  "typescript.format.placeOpenBraceOnNewLineForControlBlocks": false,
  "javascript.validate.enable": true,
  "javascript.format.insertSpaceAfterCommaDelimiter": true,
  "javascript.format.insertSpaceAfterSemicolonInForStatements": true,
  "javascript.format.insertSpaceBeforeAndAfterBinaryOperators": true,
  "javascript.format.insertSpaceAfterKeywordsInControlFlowStatements": true,
  "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": false,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": false,
  "javascript.format.placeOpenBraceOnNewLineForFunctions": false,
  "javascript.format.placeOpenBraceOnNewLineForControlBlocks": false,
  "extensions.autoUpdate": true,
  "terminal.external.windowsExec": "%COMSPEC%",
  "terminal.external.osxExec": "iTerm2.app",
  "terminal.external.linuxExec": "xterm",
  "terminal.integrated.shell.linux": "/usr/local/bin/zsh",
  "terminal.integrated.shellArgs.linux": [
    "-l"
  ],
  "terminal.integrated.shell.osx": "/usr/local/bin/zsh",
  "terminal.integrated.shellArgs.osx": [
    "-l"
  ],
  "terminal.integrated.shell.windows": "cmd.exe",
  "terminal.integrated.fontFamily": "",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.lineHeight": 0,
  "terminal.integrated.cursorBlinking": false,
  "terminal.integrated.setLocaleVariables": true,
  "projectManager.showProjectNameInStatusBar": false,
  "projectManager.sortList": "Recent",
  "newFile.showPathRelativeTo": "project",
  "newFile.defaultBaseFileName": "file",
  "tslint.autoFixOnSave": true,
  "workbench.iconTheme": "ayu",
  "[markdown]": {
    "editor.quickSuggestions": true
  },
  "docthis.includeMemberOfOnClassMembers": false,
  "docthis.includeMemberOfOnInterfaceMembers": false,
  "markdown.preview.fontSize": 12,
  "markdown-toc.depthFrom": 2,
  "auto-close-tag.activationOnLanguage": [
    "html",
    "xml",
    "php",
    "blade",
    "ejs",
    "handlebars",
    "jinja",
    "javascript",
    "javascriptreact",
    "plaintext",
    "markdown",
    "razor",
    "vue",
    "liquid"
  ],
  "editor.autoIndent": false,
  "explorer.confirmDragAndDrop": false,
  "sublimeTextKeymap.promptV3Features": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.snippetSuggestions": "top",
  "editor.formatOnPaste": false,
  "workbench.colorTheme": "Ayu Mirage Bordered",
  "editor.minimap.enabled": false,
  "materialTheme.cache.workbench.settings": {
    "themeColours": "Darker",
    "accentPrevious": "Lime"
  },
  "workbench.colorCustomizations": {},
  "gitlens.codeLens.enabled": false,
  "files.trimFinalNewlines": true,
  "editor.trimAutoWhitespace": false,
  "editor.formatOnSave": false,
  "editor.formatOnType": false,
  "javascript.format.enable": false,
  "typescript.format.enable": false,
  "html.format.enable": false,
  "json.format.enable": false,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {"language": "typescript", "autoFix": true },
    {"language": "typescriptreact", "autoFix": true },
    {"language": "vue", "autoFix": true }
  ],
  "gitlens.keymap": "alternate",
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.historyExplorer.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "diffEditor.ignoreTrimWhitespace": false,
  "files.eol": "\n",
  "typescriptHero.imports.multiLineWrapThreshold": 120,
  "breadcrumbs.enabled": true,
  "typescriptHero.imports.organizeOnSave": true
}
